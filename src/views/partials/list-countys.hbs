{{#each countys}}
  {{#if @first}}
    <div class="columns">
  {{/if}}
  <div class="column is-one-third">
    <div class="box box-link-hover-shadow">
      <h2 class="title">
        {{county}} car dealers
      </h2>
      <a href="/county/{{_id}}" class="button">
        <span class="icon is-small">
          <i class="fas fa-folder-open"></i>
        </span>
      </a>
      <a href="/dashboard/deletecounty/{{_id}}" class="button">
        <i class="fas fa-trash"></i>
      </a>
    </div>
  </div>
  {{#if @last}}
    </div>
  {{else}}
    {{#modAdd1 @index 3}}
      </div>
      <div class="columns">
    {{/modAdd1}}
  {{/if}}
{{/each}}